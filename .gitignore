#Ignore VS Code settings
.vscode/settings.json

# Ignore directories generated by Composer
/drush/contrib/
/vendor/
/web/core/
/web/modules/contrib/
/web/themes/contrib/
/web/profiles/contrib/
/web/libraries/

# Ignore sensitive information
/web/sites/*/settings.php
/web/sites/*/settings.local.php
/web/sites/default/services.yml
/web/.htaccess

# For now, include user files in git except the automatic css/js/php files
# Once production is ready, we can go back to ignoring these and let stage_file_proxy do the sync from production 
# /web/sites/*/files/
/web/sites/*/files/css
/web/sites/*/files/js
/web/sites/*/files/php
/web/sites/*/files/.htaccess
web/sites/default/files/*/sync/.htaccess

# Ignore SimpleTest multi-site environment.
/web/sites/simpletest

# Ignore files generated by PhpStorm
/.idea/

# Ignore .env files as they are personal
/.env

# Ignore certs for SP/SSO/SAML
/certs/

# Ignore folders created by Drupal Console
/console/
